import sandbox

(options, modules) = sandbox.options()
for module in modules:
    globals()[module] = __import__(module)
    print globals()['regexp_comment']
    print 'globals()[%(module)s] = __import__(%(module)s)' % locals()

if options['readline']:
    import sandbox.rl
if options['readline2']:
    import sandbox.rl2
if options['yaml']:
    import yaml
if options['os']:
    import os
if options['sys']:
    import sys
if options['re']:
    import re
